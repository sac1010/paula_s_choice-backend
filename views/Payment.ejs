<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel = "icon" href = "https://cdn.shopper.com/images/store-logos/paulaschoice-com-au-1554772002099.png"  type = "image/x-icon"> 
    <title>Shop Paula's Choice</title>
    <link rel="stylesheet" href="./styles/Payment.css" />
  </head>
  <body>
    <div id="RLeft">
      <div id="mHead">
        <div id="Rhead">
          <a href="\home">
            <h2>PAULA‚ùúS CHOICE</h2>
            <h4>SKINCARES</h4>
          </a>
        </div>
        <div id="steps">
          <a href="Shipping.html">
            <div id="1step">
              <div id="cl">1</div>
              <p>SHIPPING</p>
            </div>
          </a>
          <hr />
          <div id="Pstep">
            <div id="cl">2</div>
            <p>PAYMENT</p>
          </div>
          <hr />
          <div id="3step">
            <div id="secCl">3</div>
            <p style="color: lavender">REVIEW</p>
          </div>
          <hr />
          <div id="4step">
            <div id="secCl">4</div>
            <p style="color: lavender">COMPLETE</p>
          </div>
        </div>
      </div>
      <!--Left Head-->

      <div id="Rmain">
        <h2 style="font-weight: normal">Payment</h2>
        <hr />
        <br />
        <form id="Rmyform">
          <div>
            <input type="radio" id="paypl" name="drone" value="dewey" />
            <label for="paypl" style="margin-top: 9px; font-size: large"
              >PayPl</label
            >
            <img
              src="data:image/png;base64,"
              alt="p=PayPal"
              style="padding: 2px"
            />
          </div>
          <br /><br />
          <div>
            <input type="radio" id="huey" name="drone" value="huey" checked />
            <p style="font-size: large; line-height: 2px">Credit Card</p>
            <br /><br /><br /><br />
            <img
              src="data:image/png;base64,"
              alt="visa"
            />
            <img
              src="https://i.pinimg.com/736x/ca/0c/70/ca0c7039ddcf224cb6b075cb59e4677e.jpg"
              alt="master"
            />
            <img
              src="https://www.pngitem.com/pimgs/m/274-2746824_bhim-icon-png-image-free-download-searchpng-bhim.png"
              alt="upi"
            />
            <img
              src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcT03Z03Z1_dEH-1pSA7IOYAp7_D-FSAEZtUWg&usqp=CAU"
              alt=""
            />
          </div>
          <label><p>Card Number</p></label>
          <input type="number" id="cardNo" placeholder="1234 5678 9012 3456" />
          <div>
            <div>
              <label><p>Exp Month / Exp Year</p></label>
              <input type="month" id="expMonth" placeholder="" />
            </div>
            <div>
              <label><p style="padding-left: 23px">CVV</p></label>
              <input type="number" id="cvv" placeholder="3 digits" />
            </div>
          </div>

          <input type="submit" value="CONTINUE TO REVIEW" id="Rbtn" />
        </form>
      </div>
      <div id="terms">
        <p>
          By continuing, you agree to our <a href="/">Terms of use.</a>For more
          informationn about our privacy practics, plese see our
          <a href="/">Privicy Policy.</a>
        </p>
        <img
          src="https://www.paulaschoice.com/on/demandware.static/-/Library-Sites-paulachoice/default/dw3b046b8b/mcafee.png"
          alt=""
        />
      </div>
    </div>

    <div id="Rright">
      <div class="flex">
        <div>Order Summary</div>
        <div class="flex summary">
          <div id="cartNum">1 item</div>
          <div id="estTotal1" class="bold">112$</div>
        </div>
      </div>
      <hr />
      <div>Edit bag</div>
      <hr />
      <div id="scontainer"></div>
      <hr />
      <div class="flex">
        <div class="bold">subtotal</div>
        <div id="subtotal">112$</div>
      </div>
      <div class="flex">
        <div class="bold">shipping</div>
        <div id="shipping">0$</div>
      </div>
      <hr />
      <div class="flex">
        <div class="bold">Est. Total</div>
        <div class="bold" id="estTotal">112$</div>
      </div>
      <hr />
    </div>
  </body>
</html>
<script src="./components/Payment.js"></script>